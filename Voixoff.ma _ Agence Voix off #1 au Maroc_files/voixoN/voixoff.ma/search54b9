<script>

            function filter() {  
             var language =  document.getElementById('language').value;
             var gender =  document.getElementById('gender').value;


if(language != "vido" && gender != "vide" )
{
                const map = {

arabic: '/search?language=1'
,frensh: '/search?language=2'
,fusha: '/search?language=3'
,english: '/search?language=4'
,spain: '/search?language=6'
,amazigh: '/search?language=5'
,female: 'female'
,male:'male'
}
$("#filter_error").empty();
              
  
  location.href = map[document.getElementById('language').value]+map[document.getElementById('gender').value];
            }
            else if(language != "vido" &&  gender == "vide"){


const map = {
arabic: '/search?language=1'
,frensh: '/search?language=2'
,fusha: '/search?language=3'
,english: '/search?language=4'
,spain: '/search?language=6'
,amazigh: '/search?language=5'
}
$("#filter_error").empty();

  location.href = map[document.getElementById('language').value];

            }
            else{



$("#filter_error").html("Please select language");



            }

}  



</script>
<script>
function onlyPlayOneIn(container) {
  container.addEventListener("play", function(event) {
  audio_elements = container.getElementsByTagName("audio")
    for(i=0; i < audio_elements.length; i++) {
      audio_element = audio_elements[i];
      if (audio_element !== event.target) {
        audio_element.pause();
      }
    }
  }, true);
}

document.addEventListener("DOMContentLoaded", function() {
  onlyPlayOneIn(document.body);
});
</script>
        
        


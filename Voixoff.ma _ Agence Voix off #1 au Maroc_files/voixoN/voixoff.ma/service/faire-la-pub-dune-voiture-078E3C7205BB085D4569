                if (message) {
                    let textarea = document.getElementById('live-chat-message-textarea');
                    if (textarea) {
                        textarea.removeAttribute('readonly');
                        textarea.value = decodeURIComponent(message);
            // If needed, also enable the send button here
                        let sendButton = document.querySelector('button[disabled]');
                        if (sendButton) {
                            sendButton.removeAttribute('disabled');
                        }
                    }
                }
            });


        </script>

        

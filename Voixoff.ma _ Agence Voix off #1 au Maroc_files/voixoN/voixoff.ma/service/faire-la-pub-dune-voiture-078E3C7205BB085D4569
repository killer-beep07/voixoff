                            loop         : true,
                            mute         : true
                        },
                    } ).mount(window.splide.Extensions);
                }
            }
        },

                // Init alpinejs
        initialize() {
            var _this = this;

                    // Wait until page loaded
            document.addEventListener( 'DOMContentLoaded', function () {

                        // Initialize carousel
                _this.initMainCarousel();

